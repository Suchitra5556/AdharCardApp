package com.employee.service;

import java.util.List;

import org.springframework.data.crossstore.ChangeSetPersister.NotFoundException;

import com.employee.entity.Employee;

public interface EmployeePositionaService {
	
	Employee saveEmployee(Employee employee);
	List<Employee> saveEmployee(List<Employee> employee);
	
	
	
	List<Employee> getAlllistOfEmp(String name) throws NotFoundException;
	
	List<Employee> getAllEmp();
	
	Employee getByIdAndName(Integer id ,String name);
	
	List<Employee> getByNameOrAddress(String name , String address);

}
