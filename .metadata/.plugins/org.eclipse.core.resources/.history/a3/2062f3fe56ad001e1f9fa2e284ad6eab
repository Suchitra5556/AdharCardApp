package com.adhar.service;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Service;

import com.adhar.entity.AdharEntity;
import com.adhar.repository.AdharRepository;

@Service
public interface AdharService {
	
	@Autowired 
	private AdharRepository adharRepo;
	
	ResponseEntity<AdharEntity> save();
	ResponseEntity<AdharEntity> getAll();
	

}
